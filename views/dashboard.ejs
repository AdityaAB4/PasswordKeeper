<h1 class="mt-4">Dashboard</h1>




<form action="/dashboard" method="POST" autocomplete="off">
    <input type="hidden" name="_id" value="<%= this._id %>">
    <div class="form-group">
        <label > User Name</label>
        <input type="text" class="form-control" name="username" placeholder="Username" value="<%= users.username %>">
        <div class="text-danger">
            
        </div>
    </div>
    <div class="form-group">
        <label > Password </label>
        <input type="text" class="form-control" name="userpassword" placeholder="Password" value="<%= users.userpassword %>">
         <div class="text-danger">
            
        </div>
    </div>
  
    <div class="form-group">
        <button type="submit" class="btn btn-info"><i class="fa fa-database">Submit</i></button>
        <a class="btn btn-secondary" href="/list"><i class="fa fa-list-alt"></i>View All</a>
    </div>
</form>

<a href="/users/logout" class="btn btn-secondary">Logout</a>

<%- include ("./css"); %>
